      "sha": "b5113e13673f88fe8812c74d7efa301e78456807",
      "filename": "gnulib/import/fd-safer.c",
      "status": "modified",
      "additions": 2,
      "deletions": 2,
      "changes": 4,
      "blob_url": "https://github.com/bminor/binutils-gdb/blob/c0c3707ff46ccfb78ea175dd42d628d8c90dca8b/gnulib/import/fd-safer.c",
      "raw_url": "https://github.com/bminor/binutils-gdb/raw/c0c3707ff46ccfb78ea175dd42d628d8c90dca8b/gnulib/import/fd-safer.c",
      "contents_url": "https://api.github.com/repos/bminor/binutils-gdb/contents/gnulib/import/fd-safer.c?ref=c0c3707ff46ccfb78ea175dd42d628d8c90dca8b",
